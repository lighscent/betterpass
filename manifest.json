{
    "manifest_version": 3,
    "name": "BetterPass",
    "version": "1.0",
    "description": "Génère des mots de passe sécurisés",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": [
        "clipboardWrite"
    ]
}